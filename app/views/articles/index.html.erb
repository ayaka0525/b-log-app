<div class="container">
<!--「_tabsを読み込み」-->
<%= render 'tabs'%>

  <!-- eachメソッドを使い、データを繰り返し取得する endを忘れずに -->
  <% @articles.each do |article| %>
   <%= render 'article', article: article %>
  <% end %> <!-- each ブロックの終了 -->
<%= link_to new_article_path do %>
    <div class="float_btn">
      <i class="fa fa-plus"></i>
    </div>
  <% end %>

</div>